{
  "name": "blog",
  "version": "1.0.0",
  "description": "A pure JavaScript blog, made for practice purposes.",
  "main": "app.js",
  "dependencies": {
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "fetch": "^1.1.0",
    "highlight.js": "^9.11.0",
    "memory-cache": "^0.1.6",
    "promise": "^7.1.1",
    "riot": "^3.4.2",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "chokidar-cli": "^1.2.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-riot": "^1.1.0",
    "rollup-plugin-uglify": "^1.0.2"
  },
  "build": "webpack",
  "scripts": {
    "start": "npm run watch & npm run server",
    "build": "rollup -c",
    "watch": "chokidar 'src/**' -c 'npm run build'",
    "server": "browser-sync start --files 'src/client/*, src/index.html' --server"
  },
  "keywords": [
    "JavaScript",
    "Blog"
  ],
  "author": "William Bergqvist",
  "license": "ISC"
}
